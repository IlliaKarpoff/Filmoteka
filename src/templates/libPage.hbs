{{#each this}}
<li class="lib-list__item">
  <img class="lib-list__item-img" src="https://image.tmdb.org/t/p/w780{{backdrop_path}}" alt="{{original_title}}"
    data-id="{{id}}">
  <p class="lib-list__item-text">{{original_title}} ({{release_date}})</p>
  <p class="lib-list__item-rating">{{vote_average}}</p>
  {{!-- <p>id = {{id}}</p> --}}
</li>
{{/each}}